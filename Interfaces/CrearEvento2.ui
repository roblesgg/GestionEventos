/* --- Estilo General --- */
QWidget {
    font-family: 'Segoe UI', 'Arial', 'sans-serif';
    background-color: #F8F9FA; /* Un blanco roto muy suave */
    color: #333333;
}

/* --- Título Principal --- */
QLabel[objectName="TitleLabel_Participants"] {
    font-size: 24pt;
    font-weight: bold;
    color: #2C3E50; /* Un azul oscuro */
    padding-bottom: 10px;
    background-color: transparent;
}

/* --- Etiquetas de listas --- */
QLabel {
    font-size: 12pt;
    font-weight: bold;
    color: #333;
    background-color: transparent;
}

/* --- Botón de Atrás --- */
QToolButton {
    font-size: 16pt;
    font-weight: bold;
    color: #555555;
    background-color: transparent;
    border: none;
    padding-right: 10px;
}
QToolButton:hover {
    color: #000000;
}

/* --- Botones Principales (Añadir/Finalizar) --- */
QPushButton {
    background-color: #E0EFFF; /* Azul pastel */
    color: #2C3E50;
    font-size: 14pt; /* Letra grande como en el original */
    font-weight: bold;
    border: none;
    border-radius: 8px; /* ¡ESQUINAS REDONDEADAS! */
    padding: 12px 18px;
    min-height: 28px;
}
QPushButton:hover {
    background-color: #C6DFFF;
}
QPushButton:pressed {
    background-color: #B0D0FF;
}

/* --- Botón Mover a Preferencia (V) --- */
QPushButton[objectName="MoveToPreference_Btn"] {
    background-color: #E8F5E9; /* Verde pastel */
    color: #388E3C; /* Verde oscuro */
    font-size: 12pt;
    font-weight: bold;
}
QPushButton[objectName="MoveToPreference_Btn"]:hover {
    background-color: #C8E6C9;
}

/* --- Botón Mover a No Acepta (X) --- */
QPushButton[objectName="MoveToAvoid_Btn"] {
    background-color: #FFEBEE; /* Rojo pastel */
    color: #D32F2F; /* Rojo oscuro */
    font-size: 12pt;
    font-weight: bold;
}
QPushButton[objectName="MoveToAvoid_Btn"]:hover {
    background-color: #FFCDD2;
}

/* --- Botón Eliminar de lista --- */
QPushButton[objectName="RemoveFromList_Btn"] {
    background-color: #F1F3F5; /* Gris claro */
    color: #555; /* Gris oscuro */
    font-size: 11pt;
    font-weight: bold;
}
QPushButton[objectName="RemoveFromList_Btn"]:hover {
    background-color: #E0E0E0;
}


/* --- Campo de Texto (Nombre) --- */
QLineEdit {
    background-color: #FFFFFF;
    border: 1px solid #CED4DA; /* Borde gris claro */
    border-radius: 5px;
    padding: 8px;
    font-size: 11pt; /* Letra 11pt como en el original */
    min-height: 20px; /* Ajuste para el min-height original */
}
QLineEdit:focus {
    border: 1px solid #80bdff; /* Borde azul al seleccionar */
}

/* --- Listas --- */
QListWidget {
    background-color: #FFFFFF;
    border: 1px solid #CED4DA;
    border-radius: 5px;
    padding: 5px;
    font-size: 10pt;
}
QListWidget::item:hover {
    background-color: #F1F3F5; /* Resaltar item al pasar el ratón */
}
QListWidget::item:selected {
    background-color: #E0EFFF; /* Resaltar item seleccionado */
    color: #2C3E50;
}
